<h3>New Category</h3>
<%= render partial: "shared/error", locals: {obj: @category} %>
<%= form_for @category do |form| %>
    <div>
      <%= form.label :name %>
    </div>
    <div>
      <%= form.text_field :name %>
    </div>
    <div>
      <%= form.submit "Create category" %>
    </div>
<% end %>