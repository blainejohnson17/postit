<div class="fluid-row">
  <%= render 'shared/content_title', title: 'All Posts' %>
</div>
<div class="fluid-row">
  <div class="span10">
    <% @posts.each do |post| %>
      <%= render 'post', post: post %>
    <% end %>
    <%= will_paginate @posts, renderer: BootstrapPagination::Rails %>
  </div>
</div>