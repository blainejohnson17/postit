<%= render "shared/error", obj: @user %>

<%= form_for(@user) do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
  <%= form.submit @user.new_record? ? "Register" : "Update" %>
  </div>
<% end %>